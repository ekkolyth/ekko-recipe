#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mikekenway/Development/ekko-recipe/node_modules/.pnpm/@docusaurus+core@3.9.2_@mdx-js+react@3.1.1_@types+react@19.2.7_react@19.2.0__react-dom@_c91df3fceacf51ae1658badce1f78fc2/node_modules/@docusaurus/core/bin/node_modules:/Users/mikekenway/Development/ekko-recipe/node_modules/.pnpm/@docusaurus+core@3.9.2_@mdx-js+react@3.1.1_@types+react@19.2.7_react@19.2.0__react-dom@_c91df3fceacf51ae1658badce1f78fc2/node_modules/@docusaurus/core/node_modules:/Users/mikekenway/Development/ekko-recipe/node_modules/.pnpm/@docusaurus+core@3.9.2_@mdx-js+react@3.1.1_@types+react@19.2.7_react@19.2.0__react-dom@_c91df3fceacf51ae1658badce1f78fc2/node_modules/@docusaurus/node_modules:/Users/mikekenway/Development/ekko-recipe/node_modules/.pnpm/@docusaurus+core@3.9.2_@mdx-js+react@3.1.1_@types+react@19.2.7_react@19.2.0__react-dom@_c91df3fceacf51ae1658badce1f78fc2/node_modules:/Users/mikekenway/Development/ekko-recipe/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mikekenway/Development/ekko-recipe/node_modules/.pnpm/@docusaurus+core@3.9.2_@mdx-js+react@3.1.1_@types+react@19.2.7_react@19.2.0__react-dom@_c91df3fceacf51ae1658badce1f78fc2/node_modules/@docusaurus/core/bin/node_modules:/Users/mikekenway/Development/ekko-recipe/node_modules/.pnpm/@docusaurus+core@3.9.2_@mdx-js+react@3.1.1_@types+react@19.2.7_react@19.2.0__react-dom@_c91df3fceacf51ae1658badce1f78fc2/node_modules/@docusaurus/core/node_modules:/Users/mikekenway/Development/ekko-recipe/node_modules/.pnpm/@docusaurus+core@3.9.2_@mdx-js+react@3.1.1_@types+react@19.2.7_react@19.2.0__react-dom@_c91df3fceacf51ae1658badce1f78fc2/node_modules/@docusaurus/node_modules:/Users/mikekenway/Development/ekko-recipe/node_modules/.pnpm/@docusaurus+core@3.9.2_@mdx-js+react@3.1.1_@types+react@19.2.7_react@19.2.0__react-dom@_c91df3fceacf51ae1658badce1f78fc2/node_modules:/Users/mikekenway/Development/ekko-recipe/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@docusaurus/core/bin/docusaurus.mjs" "$@"
else
  exec node  "$basedir/../@docusaurus/core/bin/docusaurus.mjs" "$@"
fi
